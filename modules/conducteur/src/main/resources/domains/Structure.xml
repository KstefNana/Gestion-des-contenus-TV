<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:schemaLocation="http://axelor.com/xml/ns/domain-models
  https://axelor.com/xml/ns/domain-models/domain-models_6.1.xsd">

    <module name="conducteur" package="com.axelor.conducteur.db"/>

    <entity name="Structure" cacheable="true">
        <string name="name" title="structure" required="true" unique="true" />
        <date name="dateCreation"/>
        <string name="description"/>
        <one-to-one name="logo" ref="com.axelor.meta.db.MetaFile" />
    </entity>

</domain-models>